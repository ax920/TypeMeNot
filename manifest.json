{
  "name": "TypeMeNot",
  "version": "1.0",
  "description": "Real-time toxicity checker",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://localhost:3000/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/typemenot16.png",
      "32": "images/typemenot32.png",
      "48": "images/typemenoticon.ico",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/typemenot16.png",
    "32": "images/typemenot32.png",
    "48": "images/typemenoticon.ico",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}